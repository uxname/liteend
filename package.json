{
  "name": "liteend",
  "version": "0.0.1",
  "description": "Lightweight, fast, and easy to use backend app template for Node.js",
  "author": "uxname",
  "private": true,
  "license": "MIT",
  "scripts": {
    "________________ BUILD AND RUN ________________": "",
    "save-commit-info": "tsx src/common/git-commit-saver.ts",
    "build": "rimraf dist && npm run save-commit-info && run-p lint build:nest",
    "build:nest": "nest build",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "prestart:prod": "npm run db:migrations:apply",
    "start:prod": "cross-env NODE_ENV=production node --enable-source-maps dist/src/main",
    "________________ FORMAT AND LINT ________________": "",
    "lint": "biome check",
    "lint:md": "markdownlint '**/*.md' --ignore node_modules --fix --disable MD013",
    "lint:fix": "biome check --write",
    "lint:fix:unsafe": "biome check --write --unsafe",
    "ts:check": "tsc --noEmit",
    "knip": "knip --production",
    "check": "run-p ts:check lint knip lint:md",
    "________________ TEST ________________": "",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:e2e": "vitest run --config ./vitest.config.ts --dir test",
    "________________ DATABASE ________________": "",
    "db:push": "prisma db push && npm run db:gen",
    "db:reset": "prisma migrate reset --force && npm run db:push && npm run db:gen",
    "db:migrations:apply": "prisma migrate deploy && npm run db:gen",
    "db:migrations:create": "prisma migrate dev --create-only",
    "db:schema:format": "prisma format",
    "db:gen": "prisma generate --no-hints",
    "db:studio": "prisma studio --browser none --port 5555",
    "db:seed": "tsx prisma/seed.ts",
    "________________ OTHER ________________": "",
    "postinstall": "npm run prepare",
    "prepare": "lefthook install",
    "update": "npm-check-updates -u && rimraf node_modules package-lock.json && npm i --legacy-peer-deps",
    "postupdate": "npm run lint:fix && npm run check"
  },
  "dependencies": {
    "@bull-board/api": "^6.14.2",
    "@bull-board/fastify": "^6.14.2",
    "@bull-board/nestjs": "^6.14.2",
    "@dotenvx/dotenvx": "^1.51.1",
    "@fastify/compress": "^8.3.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/multipart": "^9.3.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.3.0",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/graphql": "^13.2.0",
    "@nestjs/mercurius": "^13.2.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/websockets": "^11.1.9",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "altair-fastify-plugin": "^8.4.1",
    "bullmq": "^5.65.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cross-env": "^10.1.0",
    "fastify": "^5.6.2",
    "graphql": "^16.12.0",
    "graphql-query-complexity": "^1.1.0",
    "graphql-type-json": "^0.3.2",
    "graphql-ws": "^6.0.6",
    "ioredis": "^5.8.2",
    "jwks-rsa": "^3.2.0",
    "mercurius": "^16.6.0",
    "mqemitter-redis": "^7.2.0",
    "nestjs-i18n": "^10.5.1",
    "nestjs-pino": "^4.5.0",
    "nestjs-zod": "^5.0.1",
    "ofetch": "^1.5.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "pino-roll": "^4.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@jest/types": "^30.2.0",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.9",
    "@swc/core": "^1.15.3",
    "@types/ioredis": "^5.0.0",
    "@types/node": "^24.10.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.15.6",
    "knip": "^5.70.2",
    "lefthook": "^2.0.4",
    "markdownlint-cli": "^0.46.0",
    "npm-check-updates": "^19.1.2",
    "npm-run-all": "^4.1.5",
    "pactum": "^3.8.0",
    "prisma": "^7.0.1",
    "rimraf": "^6.1.2",
    "tsconfig-paths": "4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "unplugin-swc": "^1.5.9",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.14"
  }
}
