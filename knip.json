{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/main.ts!"],
  "project": ["src/**/*.ts!", "test/**/*.ts", "!src/db-backup-tool/**"],
  "ignore": ["src/@generated/**", "src/common/git-commit-saver.ts"],
  "ignoreDependencies": [
    "cross-env",
    "pino-pretty",
    "pino-roll",
    "graphql-ws",
    "@dotenvx/dotenvx",
    "@prisma/client"
  ],
  "paths": {
    "@/*": ["src/*"]
  }
}
